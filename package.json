    "lodash.capitalize": "^4.2.1",
    "lodash.foreach": "^4.5.0",
    "lodash.map": "^4.6.0",
    "lodash.replace": "^4.1.4",
    "lodash.snakecase": "^4.1.1",
    "lodash.toupper": "^4.1.2",
    "node-fetch": "^2.6.0",
    "winston": "^3.2.1"
  },
